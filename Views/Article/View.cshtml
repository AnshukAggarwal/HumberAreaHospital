@model Hospitalproject.Models.Article

@{
    ViewBag.Title = "View";
}

<h2>View Article</h2>

<div>

    <dl class="dl-horizontal">
        <dt>Title</dt>
        <dd>@Model.ArticleTitle</dd>

        <dt>Author</dt>
        <dd>@Model.Authors.AuthorFname @Model.Authors.AuthorLname</dd>

        <dt>Date</dt>
        <dd>@Model.Date</dd>

        <dt>Content</dt>
        <dd>@Model.ArticleBody</dd>

    </dl>

</div>
<a href="/Article/List">Back to List</a> 